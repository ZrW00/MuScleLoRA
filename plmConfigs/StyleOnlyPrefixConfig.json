{
    "target_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "poison_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "victim":{
        "type": "plm",
        "model": "bert",
        "path": "bert-large-uncased",
        "num_classes": 2,
        "device": "gpu",
        "max_len": 512,
        "muscleConfig":{
            "muscle":false
        },
        "baselineConfig":{
            "baseline":true,
            "adapter":false,
            "adapterConfig":{
                "bottleneck_dim":8
            },
            "prefix":true,
            "prefixConfig":{
                "prefix_token_num":8
            }
        }
    },
    "attacker":{
        "name": "base",
        "metrics": ["accuracy", "loss"],
        "train":{
            "name": "base",
            "lr": 2e-5,
            "weight_decay": 0,
            "max_grad_norm":1.0,
            "epochs": 10,
            "batch_size": 32,
            "warm_up_epochs": 0,
            "ckpt": "best",
            "save_path": "./models",
            "visualize":false,
            "attackMethod":"style",
            "defense":true
        },
        "poisoner":{
            "name": "stylebkd",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "save":false,
            "load": true,
            "template_id": 0
        }
    },
    "clean-tune": false


}