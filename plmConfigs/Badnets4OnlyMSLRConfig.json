{
    "target_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "poison_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "victim":{
        "type": "plm",
        "model": "bert",
        "path": "bert-large-uncased",
        "num_classes": 2,
        "device": "gpu",
        "max_len": 512,
        "muscleConfig":{
            "muscle":true,
            "lora":true,
            "loraConfig": {
                "lora_alpha": 32,
                "lora_dropout": 0.0,
                "r": 8
            },
            "mslr":true,
            "mslrConfig":{
                "shortcut":false,
                "freqBand": [1, 2, 3, 4, 5, 6, 7, 8, 9],
                "inner_rank": 1,
                "mslrAlpha": 6,
                "mslrDropout": 0.0
            }
        }
    },
    "attacker":{
        "name": "base",
        "metrics": ["accuracy", "loss"],
        "train":{
            "name": "base",
            "lr": 2e-5,
            "weight_decay": 0,
            "max_grad_norm":1.0,
            "epochs": 10,
            "batch_size": 32,
            "warm_up_epochs": 0,
            "ckpt": "best",
            "save_path": "./models",
            "visualize":false,
            "attackMethod":"badnets",
            "defense":true
        },
        "poisoner":{
            "name": "badnets",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": true,
            "label_dirty": false,
            "triggers": ["cf", "mn", "bb", "tq"],
            "num_triggers": 4,
            "save": false,
            "load": true
        }
    },
    "clean-tune": false


}